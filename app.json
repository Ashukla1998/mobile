      },
      "edgeToEdgeEnabled": true,
      "package": "com.ashu1998.ShareVibes"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "895f4c7b-8ee1-41f2-8b10-28702bf4765a"
      }
